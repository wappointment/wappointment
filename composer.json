{
    "name": "wappointment/wappointment",
    "description": "Appointment manager for Wordpress",
    "type": "worpdress-plugin",
    "require": {
        "php": ">=7.3",
        "illuminate/database": "8.83.*",
        "illuminate/support": "8.83.*",
        "illuminate/http": "8.83.*",
        "symfony/event-dispatcher": "5.3.*",
        "egulias/email-validator": "^3.1",
        "guzzlehttp/guzzle": "^6.3",
        "rakit/validation": "^1.4",
        "nesbot/carbon": "^2.31",
        "soundasleep/html2text": "^0.5.0",
        "pelago/emogrifier": "^6.0",
        "symfony/css-selector": "^5.3",
        "sabre/uri": "2.1.3",
        "sabre/xml": "2.1.3",
        "sabre/vobject": "^4.5",
        "illuminate/pagination": "8.83.*",
        "true/punycode": "^2.1",
        "doctrine/dbal": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Wappointment\\": "app/"
        },
        "files": ["vendors/included/swiftmailer/swiftmailer/lib/swift_required.php"]
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "config": {
        "platform": {
            "php": "7.3"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    }
}
