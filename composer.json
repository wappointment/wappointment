{
    "name": "wappointment/wappointment",
    "description": "Appointment manager for Wordpress",
    "type": "worpdress-plugin",
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "5.5.*",
        "illuminate/support": "5.5.*",
        "illuminate/filesystem": "5.5.*",
        "illuminate/http": "5.5.*",
        "symfony/event-dispatcher": "3.4.*",
        "symfony/debug": "3.4.*",
        "symfony/translation": "3.4.*",
        "guzzlehttp/guzzle": "^6.3",
        "rakit/validation": "^0.18.0",
        "nesbot/carbon": "~1.37.0",
        "soundasleep/html2text": "^0.5.0",
        "egulias/email-validator": "2.1.14",
        "doctrine/dbal": "2.5.13",
        "pelago/emogrifier": "^3.1",
        "symfony/css-selector": "3.4.*",
        "sabre/uri": "2.1.3",
        "sabre/xml": "2.1.3",
        "sabre/vobject": "4.2.2",
        "doctrine/inflector": "1.2.0",
        "doctrine/lexer": "1.0.2",
        "doctrine/annotations": "1.4.0",
        "doctrine/cache": "1.6.2",
        "doctrine/collections": "1.4.0",
        "illuminate/pagination": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Wappointment\\": "app/"
        },
        "files": ["included/swiftmailer/swiftmailer/lib/swift_required.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3"
    }
}
